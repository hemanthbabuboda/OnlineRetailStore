package com.wipro.shoppingservice.dao;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.stereotype.Repository;

import com.wipro.shoppingservice.entity.CustomerOrder;
import com.wipro.shoppingservice.entity.Order;

@Repository
public interface CustomerOrderDao extends JpaRepository<CustomerOrder, Integer> {

	List<Order> findByOrder(Integer customerId);
}
